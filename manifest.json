{
    "manifest_version": 3,
    "name": "Remove Items from YT New Music Page",
    "version": "1.1",
    "description": "Remove sections containing unwanted items from the page.",
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.svg",
            "48": "icon.svg",
            "128": "icon.svg"
        }
    }
}